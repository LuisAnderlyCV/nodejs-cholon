            <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
            <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.all.min.js"></script>


            <script>
                //
                function valor(input) {
                    // Si el campo est√° vac√≠o, establece el valor a "0"
                    if (input.value === "") {
                        input.value = "0";
                    }
                }

                //
                async function obtenerFechaActual() {
                    const apiUrlFecha = 'https://worldtimeapi.org/api/timezone/America/Lima';

                    try {
                        const responseFecha = await fetch(apiUrlFecha);
                        const dataFecha = await responseFecha.json();
                        const fecha = dataFecha.datetime.split('T')[0];

                        document.getElementById('fechallegada').setAttribute('max', fecha);
                        document.getElementById('fechallegada').value = fecha;

                        return fecha;
                    } catch (error) {
                        console.error('Error al obtener la fecha actual:', error);
                        throw error;
                    }
                }

                async function obtenerInformacionGalpon(idgalpon) {
                    const apiUrlGalpon = `/galponsuper/${idgalpon}`;

                    try {
                        const galponResponse = await axios.get(apiUrlGalpon);
                        const respuestadelquery = galponResponse.data;

                        if (respuestadelquery) {
                            document.getElementById('nombregalpon4').value = respuestadelquery.nombregalpon;
                            document.getElementById('nombregalpon1').value = respuestadelquery.idgalpon;
                            console.log("HOLA");
                        }

                        return respuestadelquery;
                    } catch (error) {
                        console.error('Error al obtener la informaci√≥n del galp√≥n:', error);
                        throw error;
                    }
                }

                async function botoningreso(idgalpon) {
                    try {
                        // Obtener la fecha actual
                        const fecha = await obtenerFechaActual();

                        // Obtener informaci√≥n del galp√≥n
                        const respuestadelquery = await obtenerInformacionGalpon(idgalpon);

                        console.log(respuestadelquery);
                    } catch (error) {
                        console.error('Error en la funci√≥n botoningreso:', error);
                    }
                }

                function registrar() {
                    // Obtener los valores de los campos de entrada
                    const idgalpon = document.getElementById('nombregalpon1').value;
                    const cantidadpollo = document.getElementById('cantidadpollo').value;
                    const fechallegada = document.getElementById('fechallegada').value;
                    const idproducto = document.getElementById('nombre_producto').value;
                    const mortalidadpollos = document.getElementById('mortalidadpollos').value;
                    const descripcion = document.getElementById('descripcion').value;

                    // Crear un objeto de datos con los valores para usar en los controolers
                    const data = {
                        idgalpon: idgalpon,
                        cantidadpollo: cantidadpollo,
                        fechallegada: fechallegada,
                        idproducto: idproducto,
                        mortalidadpollos: mortalidadpollos,
                        descripcion: descripcion,
                    };

                    // Enviar una solicitud POST a la ruta "admin/controlgalpon" utilizando Axios
                    axios.post('/supervisor/controlgalpon', data)
                        .then(function (response) {
                            // Obtener el ID del control galpon reci√©n insertado desde la respuesta
                            const idControlGalpon = response.data.idControlGalpon;

                            // Mostrar un mensaje de √©xito con SweetAlert
                            Swal.fire({
                                icon: 'success',
                                title: 'Registro exitoso',
                                text: 'El Control Galpon se realiz√≥ correctamente.',
                                timer: 2000,
                                showConfirmButton: false
                            });

                            // Redirigir al usuario despu√©s de 2 segundos (2000 ms)
                            setTimeout(function () {
                                // Realizar la operaci√≥n de actualizaci√≥n (axios.patch) con el ID del control galpon
                                axios.patch(`/supervisor/controlgalpon/${idControlGalpon}`, { /* datos de actualizaci√≥n */ })
                                    .then(function (patchResponse) {
                                        console.log("Operaci√≥n de actualizaci√≥n realizada con √©xito:", patchResponse.data);
                                        // Redirigir a la p√°gina deseada
                                        window.location.href = '/supervisor/galpon';
                                    })
                                    .catch(function (patchError) {
                                        console.error("Error en la operaci√≥n de actualizaci√≥n:", patchError);
                                    });
                            }, 2000);
                        })
                        .catch(function (error) {
                            console.error("Error en la operaci√≥n de registro:", error);
                        });
                }


                //
                function botoneditar(idgalpon) {
                    // Asignar el ID del perfil al campo oculto
                    console.log(idgalpon);
                    axios.get(`/galponsuper/${idgalpon}`)
                        .then(function (response) {
                            const respuestadelquery = response.data;

                            if (respuestadelquery) {
                                console.log(respuestadelquery);
                                // Imprimir los datos en la consola
                                document.getElementById('idgalpon1').value = respuestadelquery.idgalpon;
                                document.getElementById('nombregalpon2').value = respuestadelquery.nombregalpon;
                            }
                        })
                        .catch(function (error) {
                            console.error(error);
                        });
                }


                function postDatos() {
                    // Obtener los valores de los campos de entrada
                    const nombregalpon = document.getElementById('nombregalpon').value;

                    // Crear un objeto de datos con los valores para usar en los controolers
                    const data = {
                        nombregalpon: nombregalpon,
                    };

                    // Enviar una solicitud POST a la ruta "clientes" utilizando Axios
                    axios.post('/supervisor/galpon', data)
                        .then(function (response) {
                            // Mostrar un mensaje de √©xito con SweetAlert
                            Swal.fire({
                                icon: 'success',
                                title: 'Registro exitoso',
                                text: 'El Galpon se realiz√≥ correctamente.',
                                timer: 2000, // Cambia la duraci√≥n del mensaje si lo deseas
                                showConfirmButton: false // Oculta el bot√≥n "OK"
                            });

                            // Redirigir al usuario despu√©s de 2 segundos (2000 ms)
                            setTimeout(function () {
                                window.location.href = '/supervisor/galpon';
                            }, 2000);
                        })

                }


                function guardarDatos() {
                    // Obt√©n el valor del campo de entrada
                    // Obtener los valores de los campos de entrada
                    const idgalpon1 = document.getElementById('idgalpon1').value;
                    const nombregalpon2 = document.getElementById('nombregalpon2').value;

                    // Crear un objeto de datos con los valores para usar en los controolers
                    const data = {
                        nombregalpon2: nombregalpon2,
                    };


                    axios.put(`/supervisor/galpon/${idgalpon1}`, data)
                        .then(function (response) {
                            Swal.fire({
                                icon: 'success',
                                title: 'Registro exitoso',
                                text: 'El Galpon se realiz√≥ correctamente.',
                                timer: 2000,
                                showConfirmButton: false
                            }).then(function () {
                                // Despu√©s de que el usuario haya admin visto la notificaci√≥n, actualiza la p√°gina
                                location.reload(); // Esto recargar√° la p√°gina actual
                            });
                        })
                        .catch(function (error) {
                            console.error(error);
                        });

                }


                function botoneliminar(idgalpon) {
                    // Utiliza SweetAlert2 para mostrar una ventana modal de confirmaci√≥n
                    Swal.fire({
                        title: '¬øEst√°s seguro?',
                        text: 'Esta acci√≥n eliminar√° el Galpon con ID ' + idgalpon,
                        icon: 'warning',
                        showCancelButton: true,
                        confirmButtonText: 'S√≠, eliminar',
                        cancelButtonText: 'Cancelar'
                    }).then((result) => {
                        if (result.isConfirmed) {
                            // El usuario confirm√≥ la eliminaci√≥n, realiza la solicitud DELETE con Axios
                            axios.delete(`/supervisor/galpon/${idgalpon}`)
                                .then((response) => {
                                    // Si la solicitud DELETE se completa con √©xito, puedes manejar la respuesta aqu√≠
                                    console.log('Galpon eliminado con √©xito');
                                    // Puedes realizar acciones adicionales, como redirigir a otra p√°gina o actualizar la interfaz de usuario
                                    window.location.reload();
                                })
                                .catch((error) => {
                                    // Si hay un error en la solicitud DELETE, puedes manejarlo aqu√≠
                                    console.error('Error al eliminar Galpon:', error);
                                    // Puedes mostrar un mensaje de error o realizar otras acciones seg√∫n sea necesario
                                });
                        }
                    });
                }

                function botonveract(idgalpon) {
                    // Utiliza SweetAlert2 para mostrar una ventana modal de confirmaci√≥n
                    Swal.fire({
                        title: '¬øEst√°s seguro?',
                        text: 'Esta acci√≥n restablecera el Galpon con ID ' + idgalpon,
                        icon: 'warning',
                        showCancelButton: true,
                        confirmButtonText: 'S√≠, restablecer',
                        cancelButtonText: 'Cancelar'
                    }).then((result) => {
                        if (result.isConfirmed) {
                            // El usuario confirm√≥ la eliminaci√≥n, realiza la solicitud DELETE con Axios
                            axios.delete(`/supervisor/borragalpon/${idgalpon}`)
                                .then((response) => {
                                    // Si la solicitud DELETE se completa con √©xito, puedes manejar la respuesta aqu√≠
                                    console.log('Galpon restablecido con √©xito');
                                    // Puedes realizar acciones adicionales, como redirigir a otra p√°gina o actualizar la interfaz de usuario
                                    window.location.reload();
                                })
                                .catch((error) => {
                                    // Si hay un error en la solicitud DELETE, puedes manejarlo aqu√≠
                                    console.error('Error al restablecer Galpon:', error);
                                    // Puedes mostrar un mensaje de error o realizar otras acciones seg√∫n sea necesario
                                });
                        }
                    });
                }
                //PARA HACER QUE SEA PURA MAYUSCULA Y CON NUMEROS, ESPACIO
                function validarYConvertir(input) {
                    // Expresi√≥n regular que acepta letras, n√∫meros y espacios
                    var regex = /^[A-Za-z0-9\s]+$/;

                    if (!regex.test(input.value)) {
                        // Si el valor no cumple con la expresi√≥n regular, elimina los caracteres no permitidos
                        input.value = input.value.replace(/[^A-Za-z0-9\s]+/g, '');
                    }

                    // Convierte a may√∫sculas
                    input.value = input.value.toUpperCase();
                }


                //PARA BUSCAR EN MI TABLA
                // Obtener referencia al campo de b√∫squeda
                const campoBusqueda = document.getElementById('busqueda');

                // Obtener todas las filas de la tabla
                const filasTabla = document.querySelectorAll('tbody tr');

                // Agregar un evento de entrada al campo de b√∫squeda
                campoBusqueda.addEventListener('input', function () {
                    const consulta = campoBusqueda.value.toLowerCase();

                    // Iterar a trav√©s de las filas de la tabla y mostrar/ocultar seg√∫n la consulta
                    filasTabla.forEach(function (fila) {
                        const contenidoFila = fila.textContent.toLowerCase();
                        if (contenidoFila.includes(consulta)) {
                            fila.style.display = 'table-row';
                        } else {
                            fila.style.display = 'none';
                        }
                    });
                });

                //
                // Funci√≥n que valida y convierte a puro n√∫mero con mensaje de l√≠mite de 1500
                function validarYConvertir2(input) {
                    // Elimina caracteres no permitidos
                    input.value = input.value.replace(/[^0-9]/g, '');

                    // Convierte el valor a n√∫mero
                    var numero = parseInt(input.value, 10);

                    // Verifica si es un n√∫mero v√°lido y no es NaN
                    if (isNaN(numero)) {
                        numero = 0; // Establece a 0 si no es un n√∫mero v√°lido
                    }

                    // Muestra un mensaje si el valor es mayor a 1500
                    if (numero > 1500) {
                        alert("La capacidad m√°xima del galp√≥n es de 1500. Se ajustar√° al m√°ximo permitido.");
                        numero = 1500;
                    }

                    // Actualiza el valor en el input
                    input.value = numero;
                }
                //

                //PARA ESCRIBIR PURA MAYUSCULA
                //function convertirAMayusculas(input) {
                // input.value = input.value.toUpperCase();
                //}


